{
  "globals": {
      "__DEV__": true,
      "describe": true,
      "it": true,
      "expect": true,
      "jest": true,
      "test": true
  },
  "env": {
      "node" : true,
      "browser": true,
      "es6": true
  },
	"settings": {
		"react": {
		"createClass": "createReactClass",
		"pragma": "React",
		"version": "15.0"
		}
	},
  "parserOptions": {
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true
      }
  },
  "plugins": ["react", "compat", "react-native"],
  "extends": ["eslint:recommended", "plugin:react/recommended", "plugin:react-native/all"]
}
